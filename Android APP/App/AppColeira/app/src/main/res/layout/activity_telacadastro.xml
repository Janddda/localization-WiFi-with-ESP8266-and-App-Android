<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.mauri.appcoleira.telacadastro">

    <Button
        android:layout_width="315dp"
        android:layout_height="90dp"
        android:text="@string/botaonovocad"
        android:id="@+id/botaonovocad"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:onClick="startTelanovocadastro"/>

    <Button
        android:layout_width="315dp"
        android:layout_height="90dp"
        android:text="@string/botaoeditcad"
        android:id="@+id/botaoeditcad"
        android:layout_below="@+id/botaonovocad"
        android:layout_centerHorizontal="true"
        android:onClick="startTelaeditarcadastro"/>

    <Button
        android:layout_width="315dp"
        android:layout_height="90dp"
        android:text="@string/botaovoltar_tcad"
        android:id="@+id/botaovoltar_tcad"
        android:layout_below="@+id/botaoeditcad"
        android:layout_centerHorizontal="true"
        android:onClick="back_tc"/>

</RelativeLayout>
